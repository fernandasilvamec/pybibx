@article{ WOS:000766607700001,
Author = {Chen, Long and Qian, Lei and Zhang, Xiao and Li, Jiazheng and Zhang,
   Zhijun and Chen, Xiaoming},
Title = {Research progress on indoor environment of mushroom factory},
Journal = {INTERNATIONAL JOURNAL OF AGRICULTURAL AND BIOLOGICAL ENGINEERING},
